<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="1600.0" maxWidth="1600.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.maze.MazeController">
    <children>
        <ImageView fitHeight="720.0" fitWidth="1295.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../images/maze.png" />
            </image>
        </ImageView>

        <Label fx:id="Name" alignment="CENTER" layoutX="180.0" layoutY="24.0" prefHeight="50.0" prefWidth="100.0" style="-fx-font-size: 20;" text="Name">
            <font>
                <Font name="Arial Black" size="20.0" />
            </font>
        </Label>

        <Label fx:id="Surname" alignment="CENTER" layoutX="460.0" layoutY="24.0" prefHeight="50.0" prefWidth="158.0" style="-fx-font-size: 20;" text="Surname">
            <font>
                <Font name="Arial Black" size="20.0" />
            </font>
        </Label>

        <Label fx:id="RobotState" layoutX="870.0" layoutY="24.0" prefHeight="50.0" prefWidth="200.0" style="-fx-font-size: 20;" text="State">
            <font>
                <Font name="Arial Black" size="20.0" />
            </font>
        </Label>

        <Label fx:id="Time" layoutX="1150.0" layoutY="24.0" prefHeight="50.0" prefWidth="200.0" style="-fx-font-size: 20;" text="Time">
            <font>
                <Font name="Arial Black" size="20.0" />
            </font>
        </Label>

        <Pane fx:id="mazePane" layoutX="400.0" layoutY="150.0" prefHeight="300.0" prefWidth="300.0" style="-fx-background-color: white; -fx-border-color: black;">
        </Pane>
    </children>
</AnchorPane>
